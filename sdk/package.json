{
  "name": "@atomicbase/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Atomicbase API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate": "openapi-typescript ../api/handlers/openapi.yaml -o src/types.ts",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "atomicbase",
    "turso",
    "libsql",
    "sqlite",
    "rest",
    "api",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "openapi-typescript": "^7.0.0",
    "typescript": "^5.3.0"
  }
}
